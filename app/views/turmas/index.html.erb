
<% @usuario = current_user %>
<% if @usuario.administrador? | @usuario.coordenador? %>

<%= render partial: "admin_index" %>

<% elsif @usuario.professor? %>

<%= render partial: "professor_index" %>

<% elsif @usuario.aluno? %>

<%= render partial: "aluno_index" %>

<% else %>

<%= render partial: "users_index" %>

<% end %> 